  "scripts": {
    "build": "nest build",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main",
    "deploy": "npm install && npm run build && npm run start:prod",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch --maxWorkers=1",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --maxWorkers=1",
    "typeorm": "cross-env NODE_ENV=development npm run build && npx typeorm -d dist/db/data-source.js",
    "migration:generate": "cross-env NODE_ENV=development npm run typeorm -- migration:generate",
    "migration:run": "cross-env NODE_ENV=development npm run typeorm -- migration:run",
    "migration:revert": "cross-env NODE_ENV=development npm run typeorm -- migration:revert",
    "typeorm:stg": "cross-env NODE_ENV=staging npm run build && npx typeorm -d dist/db/data-source.js",
    "migration:run:aws:stg": "cross-env NODE_ENV=staging npm run typeorm:stg -- migration:revert",
    "migration:revert:aws:stg": "cross-env NODE_ENV=staging npm run typeorm:stg -- migration:revert"
  },
